APP_NAME=iantheloop
APP_ENV=local
# APP_KEY will be auto-generated when running startup artisan commands
APP_KEY=
APP_DEBUG=true
APP_URL=http://iantheloop.test

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=iantheloop-db
DB_PORT=3306
DB_DATABASE=looped
DB_USERNAME=guest
# Generate random password
# Example: asd324adfsadDFjkdsie34ADSF
DB_PASSWORD=

BROADCAST_DRIVER=pusher
CACHE_DRIVER=file
QUEUE_CONNECTION=database
SESSION_DRIVER=database
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Create a Mailgun account if you would like to have access
# to the forgot password functionality
# URL: https://www.mailgun.com/
MAIL_MAILER=mailgun
MAILGUN_DOMAIN=
MAILGUN_SECRET=
MAILGUN_ENDPOINT=

# You will need to signup for an s3 Bucket on Amazon Web Services
# This is a critical for functionality because this is a user sharing application
# URL: https://aws.amazon.com/s3/

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=
AWS_BUCKET=hart-looped

# These are used for laravel-websockets, critical for functionality because there is a decent
# amount of Real-Time event things happeneing
PUSHER_APP_ID= # Random Value
PUSHER_APP_KEY= # Random Value
PUSHER_APP_SECRET= # Random Value
PUSHER_APP_CLUSTER= # Directions for configuring cluster to your region https://pusher.com/docs/channels/miscellaneous/clusters/


# These ENV variables need to be the same as the corresponding ones above.
# These establish the websocket connection to the client side
MIX_PUSHER_APP_KEY= # Same as PUSHER_APP_KEY
MIX_PUSHER_APP_CLUSTER= # Same as PUSHER_APP_CLUSTER

